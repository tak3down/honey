<?xml version="1.0" encoding="UTF-8"?>
<configuration>

  <appender class="ch.qos.logback.core.ConsoleAppender" name="CONSOLE">
    <encoder>
      <pattern>%d{HH:mm:ss} %logger{36} âž¢ %msg%n</pattern>
    </encoder>
  </appender>

  <logger level="WARN" name="org.eclipse.jetty"/>

